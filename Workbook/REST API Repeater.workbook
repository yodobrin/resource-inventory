      "content": {
        "version": "KqlItem/1.0",
        "query": "{\"version\":\"CustomEndpoint/1.0\",\"data\":null,\"headers\":[],\"method\":\"GET\",\"url\":\"https://myfunctionapp-f49c933ea0.azurewebsites.net/api/ArmGateway?code=naB905Uz9zw79YJPz6YvbRfe4IulvElf05GttQReLPajAzFu_r3Hfw%3D%3D\",\"contentType\":\"text/plain\",\"urlParams\":[{\"key\":\"armRoute\",\"value\":\"/subscriptions/$subscriptions/resourceGroups/$resourceGroups/providers/Microsoft.CognitiveServices/accounts/$accounts/deployments?api-version=2023-05-01\"},{\"key\":\"resourceIds\",\"value\":\"{AzureOpenAI}\"}],\"transformers\":[{\"type\":\"jsonpath\",\"settings\":{\"tablePath\":\"$.value\",\"columns\":[]}}]}",
